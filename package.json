{
  "name": "vue-ssr-template",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run build && npm run start-server",
    "dev": "rimraf dist && cross-env NODE_ENV=development node server",
    "production": "rimraf dist && npm run build && npm run prod-server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.client.js --progress",
    "build:server": "cross-env NODE_ENV=production webpack --config config/webpack.server.js --progress",
    "start-server": "cross-env node server.js",
    "prod-server": "cross-env NODE_ENV=production node server.js",
    "deploy": "git push && git push heroku master"
  },
  "author": "Will Constable",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "vue": "^2.6.14",
    "vue-router": "^3.5.2",
    "vue-server-renderer": "^2.6.14",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "chokidar": "^3.5.2",
    "css-loader": "^6.2.0",
    "memory-fs": "^0.5.0",
    "mini-css-extract-plugin": "^2.1.0",
    "node-sass": "^6.0.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.1.0",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "browserslist": "last 3 versions"
}
